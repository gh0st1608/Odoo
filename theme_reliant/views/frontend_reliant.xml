    <template id="template_header_magazine_proyect_reliant" inherit_id="website.layout" name="Template Header Magazine" active="False">
        <xpath expr="//header//nav" position="replace">
            <!-- Topbar -->
            <div class="oe_structure" id="oe_structure_header_magazine_1"/>
            <nav id="nav-reliant" data-name="Navbar" class="navbar navbar-light o_colored_level o_cc" style="background-color:#E6E6E6">
                <div id="top_menu_container" class="container" style="background-color:#E6E6E6">
                    <!-- Brand 
                    <t t-call="website.placeholder_header_brand"/>-->
                    <div id="logo-reliant" class="d-flex justify-content-start bd-highlight align-items-center">
                        <div class="p-2 bd-highlight">
                            <a href="/" aria-current="page" id="log_main_mobile" class="list__item__enlace logo justify-content-center py-0 nuxt-link-exact-active nuxt-link-active" >
                                <img src="/theme_reliant/static/img/reliant_logo.png" alt="Logo RELIANT" class="img-fluid list__item__enlace__logo" />
                            </a>
                        </div>
                        
                    </div>
                    <div class="d-flex align-items-center ml-auto">
                        <!-- Language Selector -->
                        <t t-call="website.placeholder_header_language_selector"/>
                        <!-- Navbar Toggler -->
                        <t t-call="website.navbar_toggler">
                            <t t-set="_toggler_class" t-value="'ml-2'"/>
                        </t>
                    </div>
                    <!-- Navbar Collapse -->
                    <div id="top_menu_collapse" class="collapse navbar-collapse">
                        <t t-set="_nav_class" t-value="'text-right'"/>
                        <t t-call="website.navbar_nav">
                            <!-- Menu -->
                            <t t-foreach="website.menu_id.child_id" t-as="submenu">
                                <t t-call="website.submenu">
                                    <t t-set="item_class" t-value="'nav-item'"/>
                                    <t t-set="link_class" t-value="'nav-link'"/>
                                </t>
                            </t>
                            <!-- Sign In -->
                            <t t-call="portal.placeholder_user_sign_in">
                                <t t-set="_item_class" t-value="'nav-item'"/>
                                <t t-set="_link_class" t-value="'nav-link'"/>
                            </t>
                            <!-- User Dropdown -->
                            <t t-call="portal.user_dropdown">
                                <t t-set="_user_name" t-value="true"/>
                                <t t-set="_item_class" t-value="'nav-item dropdown'"/>
                                <t t-set="_link_class" t-value="'nav-link'"/>
                            </t>
                        </t>
                    </div>
                </div>
            </nav>
        </xpath>
    </template>

    <template id="snippet_options" inherit_id="website.snippet_options">
       <xpath expr="//div[@data-selector='#wrapwrap > header']//we-select[@data-variable='header-template']" attributes="inside">
            <we-button title="header reliant "
               data-name="Reliant"
               data-customize-website-views="theme_reliant.template_header_magazine_proyect_reliant"
               data-customize-website-variable="'Project Reliant'"
               data-img="/theme_reliant/static/img/header-reliant.png"/>
       </xpath>
        
    </template>

