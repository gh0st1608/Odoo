<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Custom Modulo">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/theme_reliant/static/css/style.css" />
        </xpath>
    </template>

    <template id="projects" name="Proyecto">
        <t t-call="website.layout">
            <section class="s_text_block pt-1  bg-secondary s_masonry_block" data-vcss="001">
            <div class="container">
                <div class="row">
                    <div class="col-6 col-sm-6 col-lg-6 reliant_ex1">
                        <div class="reliant_project_div1">
                            <p class="p_reliant_project_list_1">Our Projects</p>
                        </div>
                    </div>
                    <div class="col-6 col-sm-6 col-lg-6 reliant_ex2">
                        <div class="reliant_project_div2">
                            <p class="p_reliant_project_list_2" style="padding-bottom: 1%;">-->ALL</p>
                            <div t-foreach="projects" t-as="pro">
                                <p class="p_reliant_project_list_2"><t t-esc="pro.name"/></p>
                            </div>
                        </div>
                    </div>  
                </div>
            </div> 
            </section>
            <t t-set="a" t-value="1"/>  
            <div t-foreach="projects" t-as="pro">
                <a t-att-href="'/projects/%s' % pro.id" >
                    <t t-if="a % 2==1"> 
                        <section class="s_text_block pt-1 pb-1 bg-secondary s_masonry_block" data-vcss="001">

                            <div class="container">
                            
                                <div class="row" >
                                
                                    <div id="img-reliant-project-list-left" class="col-8 col-sm-8 col-lg-8 reliant_ex3 ">
                                        <div class="reliant_project_div3" >
                                            <span itemprop="image" t-field="pro.image" t-options='{"widget": "image"}' style="height:100%;width:100%;"/>
                                        </div>
                                        <section  style="position:absolute;z-index:2;padding:0;margin:0;border-style: solid;border-width: 2px;bottom:5%;border-color:#9cadad;margin-left:4%;width:92%;height:90%">
                                            <p class="p-list-projects" ><t t-esc="pro.name"/></p>
                                        </section>      
                                    </div>
                                    <div id="img-reliant-project-list-right" class="col-4 col-sm-4 col-lg-4 reliant_ex4" >
                                        <div class="reliant_div5" >
                                            <span   t-field="pro.image" t-options='{"widget": "image"}' style="height:100%;width:100%;"/>
                                        </div>
                                    </div> 
                                
                                </div>
                            </div>        
                        </section>
                    </t>
                    <t t-else="">
                        <section class="s_text_block pt-1 pb-1 bg-secondary s_masonry_block" data-vcss="001">

                            <div class="container">
                            
                                <div class="row" >
                                    <div id="img-reliant-project-list-right" class="col-4 col-sm-4 col-lg-4 reliant_ex4" >
                                        <div class="reliant_div5_2" >
                                            <span   t-field="pro.image" t-options='{"widget": "image"}' style="height:100%;width:100%;"/>
                                        </div>
                                    </div> 
                                    <div id="img-reliant-project-list-left" class="col-8 col-sm-8 col-lg-8 reliant_ex3 ">
                                        <div class="reliant_project_div3_2" >
                                            <span itemprop="image" t-field="pro.image" t-options='{"widget": "image"}' style="height:100%;width:100%;"/>
                                        </div>
                                        <section  style="position:absolute;z-index:2;padding:0;margin:0;border-style: solid;border-width: 2px;bottom:5%;border-color:#9cadad;margin-left:4%;width:92%;height:90%">
                                            <p class="p-list-projects" ><t t-esc="pro.name"/></p>
                                        </section>      
                                    </div>
                                </div>
                            </div>        
                        </section>
                    </t>
                </a>
                <t t-set="a" t-value="a+1"/>
            </div>
            <div class="oe_structure oe_empty"/>
        </t>
    </template>

    <template id="projects_details" name="Detalles Proyecto">
        <t t-call="website.layout">
            <section class="s_text_block pt32 pb32 bg-secondary s_masonry_block" data-vcss="001">
                <div class="container">
                    <div class="row">
                        <div class="col-9 rel_snippet5_loc">
                            <div class="rel_snippet5_1_1">
                                <p class="p_reliant_project_1"><t t-esc="project.name"/></p>
                            </div>
                        </div>
                        <div class="col-3 rel_snippet5_loc">
                            <div  class="rel_snippet5_1_2">
                                <span style="color:#9c9c9c;">Location<br></br></span>
                                <span ><t t-esc="project.location_city.name"/>, <t t-esc="project.location_country.name"/></span>

                            </div>
                        </div> 
                    </div>
                    <div class="row">
                        <div id="img-reliant-project" class="col-12 rel_snippet5_loc">
                            <div class="rel_snippet5_2_1">
                                <span itemprop="image" t-field="project.image" t-options='{"widget": "image"}' style="height:100%;width:100%;"/>
                            </div>
                        </div>    
                    </div>
                    <div class="row">
                        <div class="col-12 rel_snippet5_loc">
                                <div class="rel_snippet5_3_1" data-name="Block" >
                                    <p class=" p_reliant_project_3"><t t-esc="project.description"/></p>
                                </div>
                        </div>
                    </div> 
                </div>
            </section>
            <div class="oe_structure oe_empty"/>
        </t>
    </template>

    <template id="services" name="Servicio">
            <t t-call="website.layout">
                <section class="s_text_block pt-1  bg-secondary s_masonry_block" data-vcss="001">
                    <div class="container">
                        <div class="row">
                            <div class="col-6 rel_snippet4_loc">
                                    <div class="rel_snippet4_1_1">
                                        <p class="p_reliant_service_1">Our Services</p>
                                    </div>
                            </div>
                            <div class="col-3 rel_snippet4_loc">
                                    <div class="rel_snippet4_1_2">
                                        <p class="p_reliant_service_2">Reliant constantly works, with social responsibility, to maintain and improve its high standards in safety, speed and efficiency in mechanized development mining. We develop specialized works according to the project requirement using trackless equipment. Our presence is around the world executing services for mining companies such as Mopani Copper</p>
                                    </div>
                            </div>
                            <div class="col-3 rel_snippet4_loc">
                                    <div class="rel_snippet4_1_2">
                                        <p class="p_reliant_service_2">Mines Plc, Konkola Copper Mines Plc, Lubambe Copper Mine Ltd, Hindustan Zinc Limited and Alphamin Resources Corp.<br></br><br></br> We Are Innovative <br></br>We are Safety Conscious <br></br>We Are Committed To Quality <br></br>We Are Always Improving</p>
                                    </div>
                            </div>
                        </div>  
                    </div> 
                </section>  
                <section class="s_text_block pt-1 pb-1 bg-secondary s_masonry_block" >
                        <div class="container">
                            <div class="row" >
                                <t t-set="cont" t-value="0"/>
                                <t t-set="color" t-value="'color_class1'"/>
                                <t t-foreach="services" t-as="serv">
                                    <t t-if="cont==2">
                                        <t t-set="cont" t-value="0"/>
                                        <t t-if="color=='color_class2'">
                                            <t t-set="color" t-value="'color_class1'"/>    
                                        </t>
                                        <t t-else="">
                                        <t t-set="color" t-value="'color_class2'"/>
                                        </t>
                                    </t>
                                    <div class="col-lg-2 rel_snippet4_loc">
                                        <a t-att-href="'/services/%s' % serv.id">                                    
                                            <div t-attf-class="rel_snippet4_2_1 #{color}">
                                                <p class="p_reliant_service_title"><t t-esc="serv.name_service"/></p>
                                                <p class="p_reliant_service_description">→<br></br><t t-esc="serv.description_service"/></p>
                                            </div>
                                        </a>
                                    </div>  
                                     <t t-set="cont" t-value="cont+1"/>
                                </t>
                            </div>
                        </div>            
                </section>
                <section class="s_text_block pt-1 pb-1 bg-secondary s_masonry_block" data-vcss="001">
                    <div class="container">
                        <div class="row" >
                            <div class="col-4 rel_snippet4_loc" >
                                
                                    <div class="rel_snippet4_3_1" data-name="Block" >
                                        <a t-att-href="'/projects/'" style="text-decoration: none;">
                                        <p class="p_reliant_service_title_2">Current Projects</p>
                                        </a>                                 
                                    </div>
                                
                            </div>
                            <div class="col-4 rel_snippet4_loc" > 
                                    <div class="rel_snippet4_3_2" data-name="Block" >
                                        <img src="/theme_reliant/static/img/project.png" style="height:100%;
                                                width:100%;"  alt=""/>
                                    </div>
                            </div>
                            <div class="col-4 rel_snippet4_loc" >
                                    <div class="rel_snippet4_3_2" data-name="Block">
                                        <img src="/theme_reliant/static/img/project.png" style="height:100%;
                                            width:100%;"  alt=""/>
                                    </div>
                            </div>
                        </div>
                    </div>
                </section>
                <div class="oe_structure oe_empty"/>
            </t>
    </template>

